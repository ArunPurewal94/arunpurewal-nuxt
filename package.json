{
  "name": "@arunpurewal/nuxt",
  "version": "1.0.1",
  "description": "Nuxt Component Module made with TailwindCSS.",
  "repository": "https://github.com/ArunPurewal94/arunpurewal-nuxt.git",
  "license": "MIT",
  "type": "module",
  "private": false,
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build --stub && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish --access public && git push --follow-tags --set-upstream origin master",
    "lint": "eslint .",
    "test": "vitest run",
    "test:watch": "vitest watch"
  },
  "dependencies": {
    "@nuxt/kit": "^3.7.3",
    "clsx": "^2.0.0"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/module-builder": "^0.5.1",
    "@nuxt/schema": "^3.7.3",
    "@nuxt/test-utils": "^3.7.3",
    "@nuxtjs/tailwindcss": "^6.8.0",
    "@types/node": "^18.17.17",
    "changelogen": "^0.5.5",
    "eslint": "^8.49.0",
    "nuxt": "^3.7.3",
    "vitest": "^0.33.0"
  }
}
